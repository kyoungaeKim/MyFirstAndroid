<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kka.myfirstandroidapp">

    <uses-permission android:name="android.permission.INTERNET" />
    //인터넷 권한
    <uses-permission android:name="android.permission.CALL_PHONE" />
    //콜 권한
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".ScrollActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".IntentActivity" />
        <activity android:name=".exam.Main2Activity" />
        <activity android:name=".exam_webView.WebViewActivity" />
        <activity android:name=".exam_listView.ListViewActivity"></activity>
    </application>

</manifest>
